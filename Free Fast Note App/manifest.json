{
    "manifest_version": 3,
    "name": "Free Fast Note",
    "version": "0.1.0",
    "description": "A light, free and fast way to take notes on your web browser",
    "action": {
      "default_popup": "popup/index.html"
    },
    "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    },
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "permissions": [
      "storage"
    ]

    
  }